<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
       <!-- hr.intervent.account.wizard -->
       <record id="hr_hr_intervent_account_wizard_view" model="ir.ui.view">
            <field name="name">Intervention change account</field>
            <field name="model">hr.intervent.account.wizard</field>
            <field name="arch" type="xml">
                <form string="Analysis wizard" version="7.0">
                   <header>
                       <button string="Cancel" special="cancel" />
                       <button name="action_change" string="Force" type="object" icon="gtk-jump-to" class="oe_highlight" />
                   </header>                   
                   <group colspan="4" col="4">
                        <field name="new_account_id" />
                        <newline />
                        
                        <field name="name" colspan="4"/>
                                           
                        <field name="from_date" />
                        <field name="to_date" />
                        
                        <field name="account_id" />
                        <field name="user_id" />
                        <field name="intervent_partner_id" />
                        <field name="mode" />
                   </group>
                </form>
            </field>
      </record>

    <act_window
        id="act_force_account_change_wizard"
        name="Force account change"
        view_mode="form"
        target="new"
        res_model="hr.intervent.wizard"
        key2="client_action_multi"
        src_model="hr.analytic.timesheet"/>
    </data>
</openerp>
